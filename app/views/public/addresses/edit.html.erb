<div class="row">
  <h4><span>　配送先編集　</span></h4>
</div>


<%= form_with model: @address, url: address_path, local:true do |f| %>


<label>宛名</label>
<%= f.text_field :name %>
</div>

<label>郵便番号</label>
<%= f.text_field :postal_code %>
</div>

<label>住所</label>
<%= f.text_field :address %>
</div>

<%= f.submit '更新' %>

<% end %>
